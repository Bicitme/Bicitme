<h1>Listing Ciclista</h1>

<table class="table table-striped">
  <thead>
    <tr>
      <th>Ciclista Run</th>
      <th>Cantidad de Reparaciones</th>
      <!--<th>Cic cant den</th>-->
      <th>Telefono</th>
      <!--<th>Cic estado</th>
      <th>Usuario</th>-->
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @ciclista.each do |ciclistum| %>
      <tr>
        <td><%= ciclistum.cic_run %></td>
        <td><%= ciclistum.cic_cant_rep %></td>
        <!--<td><%= ciclistum.cic_cant_den %></td> -->
        <td><%= ciclistum.cic_fono %></td>
        <!-- <td><%= ciclistum.cic_estado %></td>
        <td><%= ciclistum.usuario_id %></td>
        <td><%= link_to 'Show', ciclistum %></td> -->
        <td><%= link_to 'Edit', edit_ciclistum_path(ciclistum) %></td>
        <!-- <td><%= link_to 'Destroy', ciclistum, method: :delete, data: { confirm: 'Are you sure?' } %></td> -->
      </tr>
    <% end %>
  </tbody>
</table>

