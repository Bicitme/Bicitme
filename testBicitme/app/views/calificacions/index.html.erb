 

<h1>Listing Calificacions</h1>

<table>
  <thead>
    <tr>
      <th>Cal puntuacion</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @calificacions.each do |calificacion| %>
      <tr>
        <td><%= calificacion.cal_puntuacion %></td>
        <td><%= link_to 'Show', calificacion %></td>
        <td><%= link_to 'Edit', edit_calificacion_path(calificacion) %></td>
        <td><%= link_to 'Destroy', calificacion, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Calificacion', new_calificacion_path %>
