<p id="notice"><%= notice %></p>

<h1>Listing Administradors</h1>

<table>
  <thead>
    <tr>
      <th>Admin run</th>
      <th>Admin cant aceptados</th>
      <th>Admin cant ban cic</th>
      <th>Admin cant ban taller</th>
      <th>Usuario</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @administradors.each do |administrador| %>
      <tr>
        <td><%= administrador.admin_run %></td>
        <td><%= administrador.admin_cant_aceptados %></td>
        <td><%= administrador.admin_cant_ban_cic %></td>
        <td><%= administrador.admin_cant_ban_taller %></td>
        <td><%= administrador.usuario_id %></td>
        <!--
        <td><%= link_to 'Show', administrador %></td> 
        <td><%= link_to 'Edit', edit_administrador_path(administrador) %></td>
        <td><%= link_to 'Destroy', administrador, method: :delete, data: { confirm: 'Are you sure?' } %></td>-->
      </tr>
    <% end %>
  </tbody>
</table>

